{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",

    "Authentication": {
        "Audience": "account",
        "TokenValidationParameters": {
            "ValidIssuers": [
                "http://localhost:18080/realms/test",
                "http://keycloak.identity:8080/realms/test"
            ]
        },
        "MetadataAddress": "http://keycloak.identity:8080/realms/test/.well-known/openid-configuration",
        "RequireHttpsMetadata": false
    },

    //"Authentication": {
    //    "Audience": "account",
    //    "ValidIssuer": "http://localhost:18080/realms/test",
    //    "MetadataAddress": "http://keycloak.identity:8080/realms/test/.well-known/openid-configuration"
    //},

    "Keycloak": {
        "HealthUrl": "http://keycloak.identity:8080/health/",
        "AuthorizationUrl": "http://localhost:18080/realms/test/protocol/openid-connect/auth"
    }
}

